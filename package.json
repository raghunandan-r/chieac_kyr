{
  "name": "nextjs-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip install -r requirements.txt && python -m uvicorn api.index:app --reload --port 8003 --host 127.0.0.1 --reload-exclude node_modules",
    "next-dev": "next dev -p 3003",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^2.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.1.10",
    "concurrently": "^8.2.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.30",
    "next": "14.2.30",
    "next-themes": "^0.4.4",
    "preact": "^10.27.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  }
}
